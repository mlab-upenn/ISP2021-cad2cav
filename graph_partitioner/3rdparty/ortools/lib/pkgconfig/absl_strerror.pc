URL: https://abseil.io/
Version: 20210324
Requires: absl_config = 20210324, absl_core_headers = 20210324, absl_errno_saver = 20210324
Libs: -L${libdir}  -labsl_strerror
Cflags: -I${includedir} -DNOMINMAX
